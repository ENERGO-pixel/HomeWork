test("Проверка сохранения данных в базу данных", () => {
  // Создаем тестовые данные для формы ввода
  const formData = {
    имя: "Иван",
    фамилия: "Иванов",
    email: "ivanov@example.com",
    сообщение: "Привет, это мой тестовый запрос"
  };

  // Вызываем функцию сохранения данных в базу данных
  saveFormDataToDatabase(formData);

  // Проверяем, что данные были сохранены в базу данных
  expect(database.getFormData()).toEqual(formData);
});